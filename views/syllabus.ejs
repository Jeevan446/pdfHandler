<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/syllabus.css">
    <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css"
    integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg=="
    crossorigin="anonymous"
    referrerpolicy="no-referrer"
  />
</head>

<body class="<%= user.role === 'admin' ? 'admin' : '' %>">
    <button id="backBtn"><i class="fa-solid fa-arrow-left" id="arrow"></i></button>
    <div class="container">
        <% if (user.role=="admin") { %>
            <a href="addsyllabus"><button id="add">Add others</button></a>
            <% } %>
      <div id="dd">
        <% syllabus.forEach(function(f){ %>
            <a href="<%= f.url %>" target="_blank">
                <div id="items">
                    <%= f.subjectname %></span>&nbsp;&nbsp;&nbsp;</span>
                        <%= f.syll %>
                </div>
            </a>
            <a href="/content/deleteothers/<%= f._id %>" id="delete"
                ><i class="fa fa-trash" aria-hidden="true"></i
              ></a>
            <% }) %>
    </div>
  </div>
    <script>
        document.getElementById("backBtn").addEventListener("click", function() {
          window.history.back(); // Navigate to the previous page
        });
      </script>
</body>

</html>